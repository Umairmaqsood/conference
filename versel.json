{
  "version": 2,
  "builds": [
    { "src": "ficse_conference/*.php", "use": "@vercel/php" },
    { "src": "ficse_conference/api/**/*.js", "use": "@vercel/node" },
    { "src": "ficse_conference/public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/ficse_conference/api/(.*)", "dest": "/ficse_conference/api/$1" },
    { "src": "/ficse_conference/(.*).php", "dest": "/ficse_conference/$1.php" },
    { "src": "/ficse_conference/(.*)", "dest": "/ficse_conference/index.php" }
  ]
}
